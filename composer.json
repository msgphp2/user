{
    "name": "msgphp/user",
    "type": "library",
    "description": "A domain layer providing basic user management",
    "keywords": [
        "ddd",
        "domain-driven-design",
        "rich domain model",
        "user",
        "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],    
     "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/corePHP/msgphp_domain"
        }
    ],
    "require": {
        "msgphp/domain": "^1.0.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "symfony/form": "^3.4.24 || ^4.4 || ^5.0",
        "symfony/messenger": "^4.4 || ^5.0",
        "symfony/security-core": "^3.4 || ^4.4 || ^5.0",
        "symfony/validator": "^3.4 || ^4.4 || ^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-main"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\User\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
